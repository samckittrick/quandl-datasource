{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "Quandl Nasdaq",
  "id": "samckittrick-quandl-datasource",
  "metrics": true,
  "info": {
    "description": "An unofficial datasource for accessing the Quandl Nasdaq data sets",
    "author": {
      "name": "Scott McKittrick",
      "url": "https://scottmckittrick.com"
    },
    "keywords": [
      "finance",
      "nasdaq",
      "quandl"
    ],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://github.com/grafana/grafana-starter-datasource"
      },
      {
        "name": "License",
        "url": "https://github.com/grafana/grafana-starter-datasource/blob/master/LICENSE"
      }
    ],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=7.0.0",
    "plugins": []
  },
  "routes": [
    {
      "path": "quandlApi",
      "url": "https://data.nasdaq.com/",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{ .SecureJsonData.apiKey}}"
        }
      ]
    }
  ]
}